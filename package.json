{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "node prisma/scripts/mergeSchemas.js && prisma generate",
    "prisma:migrate": "node mergeSchemas.js && prisma migrate dev",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start-dev": "cross-env NODE_ENV=development TZ=Asia/Seoul node dist/main",
    "start-prod": "cross-env NODE_ENV=production TZ=Asia/Seoul node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=local jest",
    "test:watch": "cross-env NODE_ENV=local jest --watch",
    "test:cov": "cross-env NODE_ENV=local jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --testPathPattern=test/e2e --watch",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=test/unit --detectOpenHandles --watch"
  },
